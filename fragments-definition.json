{
    "containerDefinitions": [
        {
            "name": "fragments",
            "image": "siusie/fragments:latest",
            "cpu": 256,
            "memoryReservation": 512,
            "links": [],
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "entryPoint": [],
            "command": [],
            "environment": [
                {
                    "name": "AWS_REGION",
                    "value": "us-east-1"
                },
                {
                  "name": "PORT",
                  "value": "8080"
                }

            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/fragments-task",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "fragments-task",
    "executionRoleArn": "arn:aws:iam::598219889830:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "compatibilities": [
        "EC2",
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "tags": []
}
